{
  "name": "ui-scene",
  "version": "1.0.0",
  "description": "UI Scene shows a functional UI in any DAO Client",
  "scripts": {
    "start": "sdk-commands start",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "export-static": "sdk-commands export-static --destination static/ --realmName UI-Scene --baseUrl https://decentraland-scenes.github.io/UI-Scene/ --commsAdapter ws-room:ws-room-service.decentraland.org/rooms/UI-Scene",
    "lint": "eslint ./**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./**/*.{ts,tsx}",
    "format": "prettier --config .prettierrc -c ./**/*.{ts,tsx}",
    "format:fix": "prettier -w --config .prettierrc -c ./**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "3.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@dcl-sdk/utils": "^1.2.8",
    "@dcl/js-runtime": "7.5.6",
    "@dcl/react-ecs": "7.5.6",
    "@dcl/sdk": "7.5.6",
    "@dcl/sdk-commands": "7.5.6",
    "dcl-npc-toolkit": "^1.2.2",
    "dcl-ui-toolkit": "^0.10.0",
    "fp-future": "^1.0.1",
    "typescript": "5.5.4"
  }
}